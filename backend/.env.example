# VentureCompass AI v2.0 - Backend Environment Configuration
# Copy this file to .env and configure with your actual values

# Database
MONGODB_URI=mongodb+srv://username:password@cluster0.mongodb.net/venturecompass?retryWrites=true&w=majority
DB_NAME=venture_compass

# Tavily API (Required)
TAVILY_API_KEY=tvly-your-api-key-here

# LLM Configuration  
LLM_PROVIDER=openai
LLM_MODEL=gpt-4o-mini
LLM_API_KEY=sk-your-openai-api-key

# Budget Controls
COST_CAP_TAVILY_CREDITS=20
RUN_CACHE_TTL_HOURS=24

# Application
ENV=dev