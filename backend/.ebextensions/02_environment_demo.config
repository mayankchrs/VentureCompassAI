option_settings:
  aws:elasticbeanstalk:application:environment:
    # Environment variables for demo deployment
    # NOTE: Set actual values via AWS EB Console or AWS CLI, not in config files
    ENV: "demo"
    MONGODB_URI: "your-mongodb-atlas-connection-string"
    DB_NAME: "venture_compass"
    TAVILY_API_KEY: "your-tavily-api-key"
    LLM_PROVIDER: "openai"
    LLM_MODEL: "gpt-4o"
    LLM_API_KEY: "your-openai-api-key"
    S3_BUCKET: "your-s3-bucket-name"
    # COST CONTROLS FOR DEMO (CRITICAL FOR $100 BUDGET)
    COST_CAP_TAVILY_CREDITS: "20"
    COST_CAP_LLM_DOLLARS: "3"
    RUN_CACHE_TTL_HOURS: "24"
    # Performance optimizations for t2.micro
    MAX_CONCURRENT_RUNS: "1"
    REQUEST_TIMEOUT: "90"
    # Demo-specific settings
    ENVIRONMENT: "demo"