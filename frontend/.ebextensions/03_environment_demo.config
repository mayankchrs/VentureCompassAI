option_settings:
  aws:elasticbeanstalk:application:environment:
    # Backend API endpoint - UPDATE THIS WITH YOUR ACTUAL BACKEND URL
    NEXT_PUBLIC_API_BASE_URL: "http://venturecompass-backend-demo.ap-south-1.elasticbeanstalk.com/api"
    NEXT_PUBLIC_ENVIRONMENT: "demo"
    # Performance optimizations for t2.micro
    NEXT_PUBLIC_MAX_CONCURRENT_REQUESTS: "2"
    # Memory optimization
    NODE_OPTIONS: "--max-old-space-size=900"
    # Enable telemetry for debugging (disable in production)
    NEXT_TELEMETRY_DISABLED: "1"